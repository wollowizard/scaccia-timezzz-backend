{
  "env": {
    "browser": true
  },
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": [
    "@typescript-eslint/eslint-plugin"
  ],
  "ignorePatterns": [ "dist/**", "node_modules/**", "**/__tests__/**"]
}
